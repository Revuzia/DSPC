{% schema %}
{
  "name": "conatct new",
  "tag": "section",
  "settings": [
    {
      "type": "text",
      "label": "Heading",
      "id": "text-heading",
      "default": "We would love to help"
    },
    {
      "type": "text",
      "label": "Heading",
      "id": "small-heading",
      "default": "CONTECT SPADE"
    },
    {
      "type": "textarea",
      "label": "Paragraph",
      "id": "text-area",
      "default": "Let us know how we can help you! Email us, check out our FAQ's, contact us or browse our store locator."
    }
  ],
  "blocks": [
    {
      "type": "contact_new",
      "name": "Contact New",
      "limit": 3,
      "settings": [
        {
          "type": "image_picker",
          "label": "Image",
          "id": "contact-image"
        },
        {
          "type": "text",
          "label": "Button Text",
          "id": "button_text"
        },
        {
          "type": "url",
          "label": "Button Url",
          "id": "button_url"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Contact New"
    }
  ]
}
{% endschema %}

