<div class="contactuss-1 contactuss">
        <div class="page-width">
            <div class="contactuss-row">
                <div class="col-md-12">
                    <h5 class="fa-25">{{ section.settings.small-heading }}</h5>
                   <h2 class="fa-120">
                    {{ section.settings.text-heading }}
                   </h2>
                   <p class="fa-25">{{ section.settings.text-area }}</p>
                </div>
            </div>
        </div>
    </div>

<div class="contactuss">
    <div class="page-width">
        <div class="contactuss-row2 pt-10">
           {% for block in section.blocks %}
            <div class="col-md-4">
             <div class="contact-img-button">
                <div class="contact-img">
                    <img src="{{ block.settings.contact-image | img_url:'268x346' }}" alt="">
                </div>
                <div class="contact-btn">
                    <a href="{{ block.settings.button_url }}">{{ block.settings.button_text }}</a>
                </div>
              </div>
            </div>
          {% endfor %}
